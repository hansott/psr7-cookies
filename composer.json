{
    "name": "hansott/psr7-cookies",
    "type": "library",
    "description": "üç™ bakes cookies for PSR-7 messages",
    "keywords": [
        "hansott",
        "psr7-cookies",
        "http-message",
        "fig",
        "cookies",
        "setcookie"
    ],
    "homepage": "https://github.com/hansott/psr7-cookies",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Ott",
            "email": "hans@iott.consulting",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0 || ^8.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "scrutinizer/ocular": "~1.1",
        "guzzlehttp/psr7": "^2.0",
        "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "HansOtt\\PSR7Cookies\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HansOtt\\PSR7Cookies\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
